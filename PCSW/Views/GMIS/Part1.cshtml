@section scripts {

    <script type="text/javascript">
        $(document).ready(function () {

            if (sessionStorage.choice == "Provincial") {
                $("#lbl_top_heading_part_1").text("Provisional Department/Institutions");
                $("#districtDDLabelRow").text("");
                $("#districtDDRow").text("");
            } else if (sessionStorage.choice == "District") {
                $("#lbl_top_heading_part_1").text("District Office");
                createDistrictDD();
            } else {
                window.location.href = "/GMIS/Index/";
            }

            

            var gazettedMaleErr = $("#numGazettedOfficersMaleErr");
            var gazettedFemaleErr = $("#numGazettedOfficersFemaleErr");
            var gazettedMaleFrom2012Err = $("#numGazettedOfficersMaleFrom2012Err");
            var gazettedFemaleFrom2012Err = $("#numGazettedOfficersFemaleFrom2012Err");

            var noneGazettedMaleErr = $("#numNoneGazettedOfficersMaleErr");
            var noneGazettedFemaleErr = $("#numNoneGazettedOfficersFemaleErr");
            var noneGazettedMaleFrom2012Err = $("#numNoneGazettedOfficersMaleFrom2012Err");
            var noneGazettedFemaleFrom2012Err = $("#numNoneGazettedOfficersFemaleFrom2012Err");

            var contractMaleErr = $("#numContractEmployeeMaleErr");
            var contractFemaleErr = $("#numContractEmployeeFemaleErr");
            var contractMaleFrom2012Err = $("#numContractEmployeeMaleFrom2012Err");
            var contractFemaleFrom2012Err = $("#numContractEmployeeFemaleFrom2012Err");

            var yearPickerPart1Err = $("#yearPickerPart1Err");
            var dd_select_officeErr = $("#dd_select_officeErr");
            var dd_select_districtErr = $("#dd_select_districtErr");
            
            var numWashroomsFemaleErr = $("#numWashroomsFemaleErr");
            var numPrayerRoomsFemaleErr = $("#numPrayerRoomsFemaleErr");
            var dd_select_typeErr = $("#dd_select_typeErr");

            function createDistrictDD() {

                //------- Label ---------
                var districtLable = document.createElement("label");
                districtLable.appendChild(document.createTextNode("District : "));
                $("#districtDDLabelRow").append(districtLable);

                //-------- Dropdown ------------
                var districtList = ["Attock", "Bahawalnagar", "Bahawalpur", "Bhakkar", "Chakwal", "Chiniot", "Dera Ghazi Khan", "Faisalabad", "Gujranwala", "Gujrat",
                                    "Hafizabad", "Jhang", "Jhelum", "Kasur", "Khanewal", "Khushab", "Lahore", "Layyah", "Lodhran", "Mandi Bahauddin", "Mianwali", "Multan", "Muzaffargarh",
                                    "Narowal", "Nankana Sahib", "Okara", "Pakpattan", "Rahim Yar Khan", "Rajanpur", "Rawalpindi", "Sahiwal", "Sargodha", "Sheikhupura", "Sialkot", "Toba Tek Singh",
                                    "Vehari"
                ];
                               
                console.log(districtList.length);
                var selectList = document.createElement("select");
                selectList.id = "dd_select_district";
                selectList.classList = "form-control";

                var option = document.createElement("option");
                option.value = "";
                option.text = "Select a District";
                selectList.appendChild(option);

                for (var j = 0 ; j < districtList.length ; j++) {
                    var option = document.createElement("option");
                    option.value = districtList[j];
                    option.text = districtList[j];
                    selectList.appendChild(option);
                }

                $("#districtDDRow").append(selectList);
            }

            $("#btn_next_part_1").click(function () {
                if (checkInputData()) {
                    
                    yearDD = document.getElementById("yearPickerPart1");
                    selectedYear = yearDD.options[yearDD.selectedIndex].value;
                    
                    officeDD = document.getElementById("dd_select_office");
                    selectedOffice = officeDD.options[officeDD.selectedIndex].value;
                    
                    if (sessionStorage.choice == "District") {
                        districtDD = document.getElementById("dd_select_district");
                        selectedDistrict = districtDD.options[districtDD.selectedIndex].value;
                    }

                    gazettedMale = $("#numGazettedOfficersMale").val();
                    gazettedFemale = $("#numGazettedOfficersFemale").val();
                    gazettedMaleFrom2012 = $("#numGazettedOfficersMaleFrom2012").val();
                    gazettedFemaleFrom2012 = $("#numGazettedOfficersFemaleFrom2012").val();

                    noneGazettedMale = $("#numNoneGazettedOfficersMale").val();
                    noneGazettedFemale = $("#numNoneGazettedOfficersFemale").val();
                    noneGazettedMaleFrom2012 = $("#numNoneGazettedOfficersMaleFrom2012").val();
                    noneGazettedFemaleFrom2012 = $("#numNoneGazettedOfficersFemaleFrom2012").val();

                    contractMale = $("#numContractEmployeeMale").val();
                    contractFemale = $("#numContractEmployeeFemale").val();
                    contractMaleFrom2012 = $("#numContractEmployeeMaleFrom2012").val();
                    contractFemaleFrom2012 = $("#numContractEmployeeFemaleFrom2012").val();

                    sessionStorage.selectedYear = selectedYear; 
                    sessionStorage.selectedOffice = selectedOffice;

                    if (sessionStorage.choice == "District") {

                        sessionStorage.selectedDistrict = selectedDistrict;
                    }
                    var dd_select_type = document.getElementById("dd_select_type");
                    sessionStorage.selectedType = dd_select_type.options[dd_select_type.selectedIndex].value;

                    sessionStorage.gazettedMale = gazettedMale;
                    sessionStorage.gazettedFemale = gazettedFemale;
                    sessionStorage.gazettedMaleFrom2012 = gazettedMaleFrom2012;
                    sessionStorage.gazettedFemaleFrom2012 = gazettedFemaleFrom2012;

                    sessionStorage.noneGazettedMale = noneGazettedMale;
                    sessionStorage.noneGazettedFemale = noneGazettedFemale;
                    sessionStorage.noneGazettedMaleFrom2012 = noneGazettedMaleFrom2012;
                    sessionStorage.noneGazettedFemaleFrom2012 = noneGazettedFemaleFrom2012;

                    sessionStorage.contractMale = contractMale;
                    sessionStorage.contractFemale = contractFemale;
                    sessionStorage.contractMaleFrom2012 = contractMaleFrom2012;
                    sessionStorage.contractFemaleFrom2012 = contractFemaleFrom2012;

                    sessionStorage.numWashroomsFemale = $("#numWashroomsFemale").val();
                    sessionStorage.numPrayerRoomsFemale = $("#numPrayerRoomsFemale").val();

                    sessionStorage.TotalNumberOfWomen = Number(gazettedFemale) + Number(noneGazettedFemale) + Number(contractFemale);
                    sessionStorage.TotalNumberOfMen = Number(gazettedMale) + Number(noneGazettedMale) + Number(contractMale);
                    sessionStorage.TotalNumberOfWomenFrom2012 = Number(gazettedFemaleFrom2012) + Number(noneGazettedFemaleFrom2012) + Number(contractFemaleFrom2012);
                    sessionStorage.TotalNumberOfMenFrom2012 = Number(gazettedMaleFrom2012) + Number(noneGazettedMaleFrom2012) + Number(contractMaleFrom2012);

                    window.location.href = '@Url.Action("Part2", "GMIS")';
                }
            });

            function checkInputData() {

                clearAllErrors();

                var yearDD = document.getElementById("yearPickerPart1");
                var yearPickerPart1 = yearDD.options[yearDD.selectedIndex].value;
                if (yearPickerPart1 == "") {

                    yearPickerPart1Err.html("<span>Please Select the Year</span>");

                    $('html, body').animate({
                        scrollTop: $("#yearPickerPart1").offset().top - 100
                    }, 500);

                    return false;
                }

                var officeDD = document.getElementById("dd_select_office");
                var dd_select_office = officeDD.options[officeDD.selectedIndex].value;
                if (dd_select_office == "") {

                    dd_select_officeErr.html("<span>Please Select the Office</span>");
                    $('html, body').animate({
                        scrollTop: $("#dd_select_office").offset().top - 100
                    }, 500);
                    return false;
                }

                if (sessionStorage.choice == "District") {
                    var districtDD = document.getElementById("dd_select_district");
                    var dd_select_district = districtDD.options[districtDD.selectedIndex].value;
                    if (dd_select_district == "") {

                        dd_select_districtErr.html("<span>Please Select the District</span>");
                        dd_select_officeErr.html("<span>Please Select the Office</span>");

                        $('html, body').animate({
                            scrollTop: $("#dd_select_district").offset().top - 100
                        }, 500);

                        return false;
                    }
                }

                var dd_select_type = document.getElementById("dd_select_type");
                var dd_select_type_value = dd_select_type.options[dd_select_type.selectedIndex].value;
                if (dd_select_type_value == "") {

                    dd_select_typeErr.html("<span>Please Select the Type</span>");

                    $('html, body').animate({
                        scrollTop: $("#dd_select_type").offset().top - 100
                    }, 500);

                    return false;
                }

                var gazettedMale = $("#numGazettedOfficersMale").val();
                if (!(/^\d+$/.test(gazettedMale)) || Number(gazettedMale) < 0) {
                    gazettedMaleErr.html("<span>Invalid Value <br/> Please Enter a valid Number</span>");

                    $('html, body').animate({
                        scrollTop: $("#numGazettedOfficersMale").offset().top - 100
                    }, 500);

                    return false;
                }

                var gazettedMaleFrom2012 = $("#numGazettedOfficersMaleFrom2012").val();
                if (!(/^\d+$/.test(gazettedMaleFrom2012)) || Number(gazettedMaleFrom2012) < 0) {
                    gazettedMaleFrom2012Err.html("<span>Invalid Value <br/> Please Enter a valid Number</span>")
                    $('html, body').animate({
                        scrollTop: $("#numGazettedOfficersMaleFrom2012").offset().top - 100
                    }, 500);
                    return false;
                }

                var gazettedFemale = $("#numGazettedOfficersFemale").val();
                if (!(/^\d+$/.test(gazettedFemale)) || Number(gazettedFemale) < 0) {
                    gazettedFemaleErr.html("<span>Invalid Value <br/> Please Enter a valid Number</span>");
                    $('html, body').animate({
                        scrollTop: $("#numGazettedOfficersFemale").offset().top - 100
                    }, 500);
                    return false;
                }

                var gazettedFemaleFrom2012 = $("#numGazettedOfficersFemaleFrom2012").val();
                if (!(/^\d+$/.test(gazettedFemaleFrom2012)) || Number(gazettedFemaleFrom2012) < 0) {
                    gazettedFemaleFrom2012Err.html("<span>Invalid Value <br/> Please Enter a valid Number</span>");
                    $('html, body').animate({
                        scrollTop: $("#numGazettedOfficersFemaleFrom2012").offset().top - 100
                    }, 500);
                    return false;
                }
                //---------------------------------------------------------------------------

                var noneGazettedMale = $("#numNoneGazettedOfficersMale").val();
                if (!(/^\d+$/.test(noneGazettedMale)) || Number(noneGazettedMale) < 0) {
                    noneGazettedMaleErr.html("<span>Invalid Value <br/> Please Enter a valid Number</span>")
                    return false;
                }

                var noneGazettedMaleFrom2012 = $("#numNoneGazettedOfficersMaleFrom2012").val();
                if (!(/^\d+$/.test(noneGazettedMaleFrom2012)) || Number(noneGazettedMaleFrom2012) < 0) {
                    noneGazettedMaleFrom2012Err.html("<span>Invalid Value <br/> Please Enter a valid Number</span>")
                    return false;
                }

                var noneGazettedFemale = $("#numNoneGazettedOfficersFemale").val();
                if (!(/^\d+$/.test(noneGazettedFemale)) || Number(noneGazettedFemale) < 0) {
                    noneGazettedFemaleErr.html("<span>Invalid Value <br/> Please Enter a valid Number</span>")
                    return false;
                }

                var noneGazettedFemaleFrom2012 = $("#numNoneGazettedOfficersFemaleFrom2012").val();
                if (!(/^\d+$/.test(noneGazettedFemaleFrom2012)) || Number(noneGazettedFemaleFrom2012) < 0) {
                    noneGazettedFemaleFrom2012Err.html("<span>Invalid Value <br/> Please Enter a valid Number</span>")
                    return false;
                }
                //---------------------------------------------------------------------------

                var contractMale = $("#numContractEmployeeMale").val();
